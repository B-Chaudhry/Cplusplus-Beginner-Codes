#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	float x[3][3] = {{1.3, 1.4, 1.7} , {5.1, 8, 9}  , {2.3, 1.8, 3.4}};
	float y[3][3];
    
	for (int i=0; i<3; i++){
	
       for (int j=0; j<3; j++) {
	   y[i][j] = {x[i][j] / x[i][i]};
}
}
		for (int i=0; i<3; i++){
	
       for (int j=0; j<3; j++ )	{
	   
    	cout << y[i][j] << " \t" ; 

}	
      cout<< endl;
}
return 0;
}
