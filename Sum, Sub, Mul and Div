#include <iostream>
#include <string>
using namespace std;

int main()
{ 
int a , b, sum, sub, mul, div;
cin >> a;
cin >> b;

 sum = a+b;
 sub= a-b;
 mul = a*b;
 div = a/b;
 cout<<"sum: "<<sum<<endl<<"subtraction: "<<sub<<endl<<"multiplication: "<<mul<<endl<<"division: "<<div;

  
 
  return 0 ;
   
}
